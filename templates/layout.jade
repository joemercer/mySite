!!! 5
block vars
  - var bodyclass = null;
html(lang='en')
  head
    block head
      title
        block title
          = locals.title
      meta(name='viewport', content='width=device-width, initial-scale=1.0')

      // Bootstrap
      link(href='css/bootstrap.min.css', rel='stylesheet', media='screen')

      // !!! THESE DON'T EXIST!!!

      // HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries
      // if lt IE 9
      // script(src="../../assets/js/html5shiv.js")
      // script(src="../../assets/js/respond.min.js")

      link(rel='alternate', href=locals.url+'/feed.xml', type='application/rss+xml', title=locals.title)

      // !!! TO DELETE

      // link(rel='stylesheet', href='http://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic|Anonymous+Pro:400,700,400italic,700italic|Merriweather:400,700,300')
      // link(rel='stylesheet', href='/css/main.css')
  body(class=bodyclass)
    div#wrap
      header.header.container
        block header
          h1
            a(href=locals.url)= locals.name
            |  - 
            a(href="http://localhost:8080/resume.html") Resume
          p.description.lead= locals.description

      div#content.container
        block content

      footer.footer.container
        block footer
          section.about
            !=contents['about.md'].html
          section.copy
            p &copy; #{ new Date().getFullYear() } #{ locals.owner } &mdash; powered by&nbsp;
              a(href='https://github.com/jnordberg/wintersmith') Wintersmith
              //- please leave the "powered by" if you use the design

    // jQuery (necessary for Bootstrap's JavaScript plugins)
    script(src="/libs/jquery-2.0.3.min.js")
    // Include all compiled plugins (below), or include individual files as needed
    script(src="js/bootstrap.min.js")